{
  "manifest_version": 2,
  "name": "lego house",
  "version": "1.0",
  "description":"Shows active contests",
  "permissions": [
   "activeTab"
   ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-2.1.3.min.js","content.js"]
  }
],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
